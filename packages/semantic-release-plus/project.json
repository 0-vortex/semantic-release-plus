{
  "root": "packages/semantic-release-plus",
  "sourceRoot": "packages/semantic-release-plus/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/semantic-release-plus",
        "tsConfig": "packages/semantic-release-plus/tsconfig.lib.json",
        "packageJson": "packages/semantic-release-plus/package.json",
        "main": "packages/semantic-release-plus/src/index.js",
        "assets": ["packages/semantic-release-plus/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/semantic-release-plus/**/*.js"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/semantic-release-plus"],
      "options": {
        "jestConfig": "packages/semantic-release-plus/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
