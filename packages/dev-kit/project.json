{
  "root": "packages/dev-kit",
  "sourceRoot": "packages/dev-kit/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/dev-kit",
        "tsConfig": "packages/dev-kit/tsconfig.lib.json",
        "packageJson": "packages/dev-kit/package.json",
        "main": "packages/dev-kit/src/index.ts",
        "assets": ["packages/dev-kit/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/dev-kit/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/dev-kit"],
      "options": {
        "jestConfig": "packages/dev-kit/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
